<Application x:Class="TodoList.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/TsukiTheme;component/Theme.xaml"/>
                
                <ResourceDictionary Source="pack://application:,,,/UI/Themes/Colours.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/UI/Images/Images.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
